{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pico Debug â€” RP2350 (FreeRTOS)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",

    "objdumpPath": "/usr/bin/arm-none-eabi-objdump",
// "nmPath": "/usr/bin/arm-none-eabi-nm",

      "executable": "${workspaceFolder}/build/main.elf",
      "gdbPath": "/usr/bin/gdb-multiarch",
      "device": "Cortex-M33",

      "serverpath": "/home/krzysiek/openocd/openocd/src/openocd",
      "searchDir": ["/home/krzysiek/openocd/openocd/tcl"],
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/rp2350.cfg"
      ],

      "gdbTarget": "localhost:50000",
      "svdFile": "${env:PICO_SDK_PATH}/src/rp2350/hardware_regs/RP2350.svd",

      "toolchainPrefix": "arm-none-eabi",
      "runToEntryPoint": "main",
      "rtos": "FreeRTOS",

      "openOCDLaunchCommands": [
        "gdb_port 50000",
        "tcl_port 50001",
        "telnet_port 50002",
        "adapter speed 5000"
      ],

      "overrideLaunchCommands": [
        "target extended-remote localhost:50000",
        "monitor adapter speed 5000",
        "monitor reset init",
        "monitor halt",
        "load"
      ],

      "postLaunchCommands": [
        "monitor halt",
        "monitor reset init"
      ],

      "showDevDebugOutput": "parsed",
      "preLaunchTask": "Build (cmake --build)"
      
    }
  ]
}
